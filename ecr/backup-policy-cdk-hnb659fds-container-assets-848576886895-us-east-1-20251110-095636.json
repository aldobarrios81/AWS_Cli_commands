{
  "Version" : "2012-10-17",
  "Statement" : [ {
    "Sid" : "LambdaECRImageRetrievalPolicy",
    "Effect" : "Allow",
    "Principal" : {
      "Service" : "lambda.amazonaws.com"
    },
    "Action" : [ "ecr:BatchGetImage", "ecr:GetDownloadUrlForLayer" ],
    "Condition" : {
      "StringLike" : {
        "aws:sourceArn" : "arn:aws:lambda:us-east-1:848576886895:function:*"
      }
    }
  } ]
}
